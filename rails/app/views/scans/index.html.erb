<h1>Listing scans</h1>

<table>
  <tr>
    <th>Value</th>
    <th>Scanned at</th>
    <th>Event</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scans.each do |scan| %>
  <tr>
    <td><%= scan.value %></td>
    <td><%= scan.scanned_at %></td>
    <td><%= scan.event_id %></td>
    <td><%= link_to 'Show', scan %></td>
    <td><%= link_to 'Edit', edit_scan_path(scan) %></td>
    <td><%= link_to 'Destroy', scan, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Scan', new_scan_path %>
