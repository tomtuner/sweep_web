<div class="page-fold">
	<div class="row">
		<div class="centered">
			<section>
				<svg class="chart">
			
				</svg>
			</section>
			<script type="text/javascript">

			


			var dataset = [ 5, 10, 15, 20, 25 ];
			var w = 500;
			var h = 50;
			var svg = d3.select("svg")
			            .attr("width", w)   // <-- Here
			            .attr("height", h); // <-- and here!

			var circles = svg.selectAll("circle")
			                 .data(dataset)
			                 .enter()
			                 .append("circle");
			 circles.attr("cx", function(d, i) {
			             return (i * 50) + 25;
			         })
			        .attr("cy", h/2)
			        .attr("r", function(d) {
			             return d;
			        })
					.attr("fill", "yellow")
					.attr("stroke", "orange")
					.attr("stroke-width", function(d) {
					    return d/2;
					});;
			</script>
			<style>
				div.bar {
				    display: inline-block;
				    width: 20px;
				    height: 75px;   /* We'll override this later */
				    background-color: teal;
					margin-right: 2px;
				}
			</style>
		</div>
	</div>
</div>